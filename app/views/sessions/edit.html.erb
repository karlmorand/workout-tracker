<h1>Edit Session</h1>

<%= form_for @session do |f| %>
  <div>
    <%= f.label :sets %>
    <%= f.text_field :sets %>
  </div>
  <div>
    <%= f.label :reps %>
    <%= f.text_field :reps %>
  </div>
  <div>
    <%= f.label :weight %>
    <%= f.text_field :weight %>
  </div>
  <div>
    <%= f.label :notes %>
    <%= f.text_area :notes %>
  </div>
  <%= f.submit "Submit Session Edits" %>
<% end %>
